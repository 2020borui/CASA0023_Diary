library(dplyr)
library(tidyverse)
library(kableExtra)
library(readr)
library(here)
library(knitr)
library(kableExtra)
library(readxl)
library(fs)
#read in data
read_excel(here("tables", "enhancement.xlsx")) %>%
# Remove newline characters (\n) and pipe symbols (|) from all columns
mutate_all(~ gsub("\n", " ", .)) %>%
mutate_all(~ gsub("\\|", "", .)) %>%
# Create the table using knitr and make it look nice with APA style
knitr::kable(booktabs = TRUE) %>%
kable_styling(position = "center", fixed_thead = TRUE, latex_options = "scale_down") %>%
# Bold the first row (header)
row_spec(0, bold = TRUE)
library(dplyr)
library(tidyverse)
library(kableExtra)
library(readr)
library(here)
library(knitr)
library(kableExtra)
library(readxl)
library(fs)
#read in data
read_excel(here("tables", "enhancement.xlsx")) %>%
# 使用 str_replace_all 来替换换行符和 | 字符
mutate_all(~ str_replace_all(., "\n", " ")) %>%  # 去掉换行符
mutate_all(~ str_replace_all(., "\\|", "")) %>%  # 去掉 | 字符
# 使用 knitr 创建表格并进行美化
knitr::kable(booktabs = TRUE) %>%
kable_styling(position = "center", fixed_thead = TRUE, latex_options = "scale_down") %>%
# 让标题行加粗
row_spec(0, bold = TRUE)
